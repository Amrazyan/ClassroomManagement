    <link href="~/Content/bootstrap.css" rel="stylesheet" />

    <link href="~/Content/css/mycss.css" rel="stylesheet" />

    <!-- <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script> -->
