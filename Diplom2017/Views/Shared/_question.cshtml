<table class="table">
    <thead>
        <tr>
            <th>Questions</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td><input type="button" onclick="Create(this)" id="@item.Id" class="btn btn-default" value="@item" /></td>
                </tr>
            }
        }
    </tbody>
</table>