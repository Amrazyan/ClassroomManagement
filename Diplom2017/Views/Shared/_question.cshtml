
@*@Model IEnumerable<Diplom2017.Models.Quest>*@

    <table class="table">
        <thead>
            <tr>
                <th>Questions</th>               
            </tr>
        </thead>
        <tbody>         
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>                      
                        <td>@item</td>
                    </tr>
                }
            }
        </tbody>
    </table>
