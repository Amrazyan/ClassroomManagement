@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">
<head>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/mycss.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Content/myJS/userScripts.js"></script>
    <script src="http://ajax.microsoft.com/ajax/jquery/jquery-1.4.2.js" type="text/javascript"></script>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Poltechnic</title>
</head>
<body>


    <div class="container" style="margin-top:10%">
        <div class="row">
            <div class="col-md-6 col-md-offset-3 col-sm-4 col-sm-offset-4 shadow">
                <div style="padding-top:6%; padding-bottom:16%">

                        <h1>@Session["my"]</h1>

                        <h1 id="h"> @*Subjects*@</h1>

                        <input type="hidden" name="userId" value="2" id="HiddenText" /> <!--change to real user id-->

                        <div id="questions" class="form-group">
                            <div style="width:100%; text-align:center">
                                <span id="read" class="glyphicon glyphicon-repeat userpageReload"></span>
                            </div>
                        </div>
                        <hr>                      
                        <div class="form-group">
                            <button type="submit" id="sendAnswer" class="btn btn-success" style="float:right"> Պատասխանել </button>
                        </div>                                        
                </div>
            </div>
        </div>
    </div>
    <script src="bootstrap/js/bootstrap.js"></script>
</body>
</html>